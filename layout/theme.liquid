    {% if template == 'index' %}
    <script>
      var hash = document.querySelectorAll('.hash');
      hash[0].addEventListener('click', function(e){ 
        e.preventDefault();
        var hash_item = this.getAttribute("href").replace('/#','') || '#';

        var item = document.getElementById(hash_item)

        var target = item
        animate(document.scrollingElement || document.documentElement, "scrollTop", "", 0, target.offsetTop, 1000, true);

      });
    </script>
    {% endif %}
    

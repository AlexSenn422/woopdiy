</head>
  <body class="template-{{ template | split: '.' | first }}">
    <div class="wrapper">
      {% include 'cart' %}
      <div id="announce-bar">
        <div class="flex-container">
          <div class="image-icon"> <img src="https://cdn.shopify.com/s/files/1/0025/0799/7284/files/shipped.png"> </div>
          <div class="description"><span>FREE SHIPPING for orders over $40</span></div>
          <div class="btn-group"> 
  <!----Turned off-----          <a class="btn-yes" href="#"></a>  ------->
  <!----Turned off-----          <a class="btn-no" href="#">No</a>  ------->
          </div>
        </div>
      </div>
      <header id="site-header">
        <div class="centered">
          <div class="header-box">
            <a href="#!" class="mobile-menu-activator" title="Menu"><span>Menu</span></a>
            <nav>
              <div class="mobile-menu-nav">
                <a href="#!" class="mobile-menu-close" title="Close">Close</a>
                <a class="cart-link" href="/cart">
                  <i class="fa fa-shopping-cart"></i>
                  <span>Cart</span>
                </a>
              </div>
              <ul>
                {% for link in linklists.main-menu.links %}
                {% if link.title == '|' %}
                <li class="separator"></li>
                {% else %}
                <li class="nav-link{%  if link.links != blank %} has_dropdown{% endif %}" ><a {%  if link.url contains '#' %} class="hash"{% endif %}href="{{ link.url | escape }}">{{ link.title | escape }}</a>
                  {% if link.links != blank %}
                  <ul class="subnav">
                    {% for childlink in link.links %}
                    <li><a href="{{ childlink.url | escape }}">{{ childlink.title | escape }}</li>
                      {% endfor %} 
                  </ul>
                  {% endif %}
                </li>
                {% endif %}
                {% endfor %}
              </ul>
              <form class="mobile-search" action="/search">
                <i class="fa fa-search"></i> <input type="text" name="q" placeholder="Search">
              </form>
            </nav>
          </div>
          <div class="header-box logo-box">
            <div class="logo-container">
              <a href="/" class="logo">
                <img src="{{ "logo.svg" | asset_url }}">
              </a>
            </div>
          </div>
          <div class="header-box">
            <div class="header-controls">
              <form class="search" action="/search">
                <i class="fa fa-search"></i> <input name="q" type="text" placeholder="Search">
              </form> 

              {% if customer  %}
              <a href="/account">Your Account</a>
              {% else %}
              {{ 'Log in' | customer_login_link }} 
              {% endif %}

              <span class="separator"></span>
              <a class="cart-link" href="/cart">
                <i class="fa fa-shopping-cart"></i> 
                <div id="cart"  class="cart-value">
                  {% if cart.item_count > 0 %}
                <span class="cart-count">{{ cart.item_count }}</span> 
                  {% endif %}
                </div>
                <span>Cart</span>
              </a>
            </div>
          </div>
        </div>
      </header>
      {{content_for_layout}}
      <footer id="site-footer">
        <div class="floating-image floating-image-lv">
          <img src="{{ 'green.png' | asset_url }}">
        </div>
        <div class="floating-image floating-image-lv-2">
          <img src="{{ 'cheese.png' | asset_url }}">
        </div>
        <div class="floating-image floating-image-lv-3">
          <img src="{{ 'mint.png' | asset_url }}">
        </div>
        <div class="centered asided love-section">
          <aside>
            <h2 class="section-title">
              <i>Why people</i>
              love<br>us?
            </h2>
          </aside>
          <aside class="points-grid">
            <div class="point">
              <div class="point-image"></div>
              <div class="point-info">
                <h3>Recipes</h3>
                <p>We are oil recipe aficionados</p>
              </div>
            </div>
            <div class="point">
              <div class="point-image"></div>
              <div class="point-info">
                <h3>Share</h3>
                <p>We share. We believe in a better world.</p>
              </div>
            </div>
            <div class="point">
              <div class="point-image"></div>
              <div class="point-info">
                <h3>Fun</h3>
                <p>We have fun!</p>
              </div>
            </div>
            <div class="point">
              <div class="point-image"></div>
              <div class="point-info">
                <h3>Like you</h3>
                <p>We are just like you</p>
              </div>
            </div>
          </aside>
        </div>
        <div class="centered">
          <div class="subscribe">
            <h2 class="section-title"><i>Subscribe To our</i> newsletter</h2>
            {%- assign formId = 'ContactFooter' -%}
            {% form 'customer', id: formId, novalidate: 'novalidate' %}
            <div class="input-group {% if form.errors %} input-group--error{% endif %}">
              {% include 'form-status', form_id: formId, success_message: 'general.newsletter_form.confirmation' %}

              <input type="hidden" name="contact[tags]" value="newsletter">
              <input type="email"
                     name="contact[email]"
                     id="{{ formId }}-email"
                     class="input-group__field newsletter__input{% if form.errors %} input--error{% endif %}"
                     value="{% if customer %}{{ customer.email }}{% endif %}"
                     aria-label="{{ 'general.newsletter_form.email_placeholder' | t }}"
                     aria-required="true"
                     required
                     autocorrect="off"
                     autocapitalize="off"
                     {% if form.errors %} aria-invalid="true" aria-describedby="{{ formId }}-email-error"{% endif %}>
              <input type="submit" class="newsletter__submit" name="commit" id="Subscribe" value="{{ 'general.newsletter_form.submit' | t }}">
              <span id="{{ formId }}-email-error" class="input-error-message">{{ form.errors.translated_fields['email'] | capitalize }} {{ form.errors.messages['email'] }}.</span>
            </div>
            {% endform %}
          </div>
          <div class="footer-middle">
            <a href="#!" class="footer-logo"><img src="{{ "logo.svg" | asset_url }}"></a>
            <nav class="footer-nav">
              <ul>
                {% for link in linklists.footer.links %}
                {% if link.title == '|' %}
                <li class="separator"></li>
                {% else %}
                <li><a href="{{ link.url | escape }}">{{ link.title | escape }}</a></li>
                {% endif %}
                {% endfor %}
              </ul>
            </nav>
          </div>
          <div class="footer-bottom">
            <div class="footer-widget">
              <h3>What is WoopDIY?</h3>
             {{ settings.footer_text_first }}
            </div>
            <div class="footer-widget">
              <h3>Keep in touch</h3>
              <p>{{ settings.footer_text_second }}</p>
              <div class="footer-icons">
                <a href="#!"><i class="fa fa-instagram" aria-hidden="true"></i></a>
                <a href="#!"><i class="fa fa-facebook-square" aria-hidden="true"></i></a>
                <a href="#!"><i class="fa fa-pinterest-square" aria-hidden="true"></i></a>
              </div>
            </div>
            <div class="footer-widget">
              <h3>Contact Us</h3>
              <p><strong>Email:</strong> <a class="mail-link" href="mailto:info@woopdiy.com">info@woopdiy.com</a></p>
            </div>
          </div>
        </div>
      </footer>
    </div>
    <script src="{{ 'custom.js' | asset_url }}" defer></script>
    <script>
      $(function(){
        ScrollReveal().reveal('.reveal-collection', { delay:200 });

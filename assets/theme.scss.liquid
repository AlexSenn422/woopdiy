.product-main-images-slider {
  width: 100%;
  box-shadow: 0 2px 0.625rem #ccc;
  border: 1px solid #ccc;
  overflow: hidden;
  border-radius: 1rem;
  
  &:not(.slick-initialized) > div {
    display: none;
    
    &:first-child {
      display: block;
    }
  }
  
  .slick-prev,
  .slick-next {
    opacity: 0;
  }    
  
  &:hover {
    .slick-prev,
    .slick-next {
      opacity: 1;
    }
  }
}

.product-main-images-slider,
.product-main-images-pager {
  .slick-prev,
  .slick-next {
    position: absolute;
    width: 2rem;
    height: 2rem;
    top: 50%;
    left: 0.7rem;
    border-radius: 50%;
    box-shadow: 0 0 0.31rem #ccc;
    z-index: 200;
    background: #fff;
    cursor: pointer;
    transition: opacity 0.5s;
    text-indent: -200px;
    overflow: hidden;
    
    &::after {
      content: '';
      width: 0.625rem;
      height: 0.625rem;
      border-style: solid;
      border-width: 1px 1px 0 0;
      border-color: #000;
      position: absolute;
      transform: rotate(225deg);
      top: 0.68rem;
      left: 0.8rem;
    }
  }
  
  .slick-next {
    left: auto;
    right: 0.7rem;
    
    &::after {
      transform: rotate(45deg);
      left: 0.55rem;
    }
  }
}

.product-main-images-pager {
  padding: 0 2rem;
  display: flex;
  justify-content: center;
  
  &:not(.slick-initialized) {
    overflow: hidden;
  }
  
  .slick-prev {
    left: 0;
  }
  
  .slick-next {
    right: 0;
  }
  
  .pager-slide {
    padding: 1rem;
    flex: 0 0 25%;
    
    @include media-query($small) {
      padding: 1rem 4px;
    }
    
    .image-ratio-container {
      border: 0.2rem solid #aaa;
      border-radius: 1rem;
      cursor: pointer;
      transition: border-color 0.5s;
      box-shadow: 0 5px 8px #ddd;
      display: block;
      max-width: 100%;
    }
  }
}




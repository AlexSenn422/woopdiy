.mini-cart-active{
  overflow:hidden;
}

#mini-cart {
  position: fixed;
  top: 0;
  bottom: 0;
  right: 0;
  width: 0;
  z-index: 1000;
  overflow: hidden;
  transition: width 0.7s;
}

.mini-cart-active #mini-cart {
  width: 100%;
}

#mini-cart .cart-inner {
  width: 650px;
  height: 100%;
  top: 0;
  background: #fff;
  position: absolute;
  right: 0;
  padding: 2rem 3rem;
  box-shadow: 0 0 30px rgba(0, 0,0, 0.15);
  transform: translateX(100%);
  transition: transform 0.5s;
  overflow-y: auto;
}

.mini-cart-active #mini-cart .cart-inner {
    transform: translateX(0);
}

#mini-cart .cart-inner .close-cart {
    position: absolute;
    right: 4rem;
    top: 2.5rem;
    font-size: 3.5rem;
    font-family: 'Montserrat', Arial, Helvetica, sans-serif;
    font-weight: 400;
    color: #666;
}

#mini-cart .cart-inner > h3 {
    font-family: 'Cursive', cursive;
    color: #7c4182;
    font-size: 2rem;
    line-height: 1.3em;
    margin-bottom: 2rem;
}

#mini-cart .cart-inner h3 i {
    margin-right: 5px;
}

#mini-cart .cart-inner .cart-item {
    border-top: 2px solid #f2f2f2;
    padding: 0.8rem 1rem;
    display: flex;
    align-items: center;
}

#mini-cart .cart-total {
    border: 2px solid #f2f2f2;
    border-width: 2px 0;
    padding: 0.8rem 1rem 0.3rem;
    text-align: right;
    font-family: 'Cursive', cursive;
    color: #7c4182;
    font-size: 1.8rem;
    line-height: 4rem;
}

#mini-cart .cart-total .total {
    font-weight: 900;
    font-family: 'Montserrat', Arial, Helvetica, sans-serif;
    color: #00b36c;
    font-size: 1.3rem;
    margin-left: 0.5rem;
}

#mini-cart .cart-inner .cart-item h3 {
    padding: 0 0 0 1rem;
    flex: 1 1 auto;
    color: #888;
    line-height: 1.3em;
    font-size: 1em;
    margin: 0;
}

#mini-cart .cart-item-image {
    display: block;
    border-radius: 10px;
    border: 2px solid #ddd;
    box-shadow: 5px 5px 12px rgba(0,0,0,0.1);
    overflow: hidden;
    width: 84px;
    height: 84px;
    flex: 0 0 88px;
}

#mini-cart .final-price-container {
